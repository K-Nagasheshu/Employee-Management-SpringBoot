package com.course.www.Todos_spb_139.todos;

import java.util.ArrayList;
import java.util.List;

import org.springframework.stereotype.Service;


@Service
public class Serv {
private static List<Student> suman = new ArrayList<>();
	
	private static int count=0;
	
	static {
		suman.add(new Student(++count, 1, "nagaraju", "ten"));
		suman.add(new Student(++count, 921, "maharani", "eight"));
		suman.add(new Student(++count, 780, "saroja", "nine"));
		suman.add(new Student(++count, 527, "shiva", "seven"));
	}
	public List<Student>findAll() {
		return suman;  
	}
	