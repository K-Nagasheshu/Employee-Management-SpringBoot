package com.course.www.Todos_spb_139.todos;

import java.util.ArrayList;
import java.util.List;

import org.springframework.stereotype.Service;


@Service
public class Serv {
private static List<Student> suman = new ArrayList<>();
	
	private static int count=0;
	
	static {
		suman.add(new Student(++count,"nagaraju",20528812,"sick", 3));
		suman.add(new Student(++count, "sanjay",20512325 ,"annual",2));
		suman.add(new Student(++count, "mounika",20524213 ,"metarnity",8));
		suman.add(new Student(++count, "megha",20528810 ,"unplan",1));
	}
	public List<Student>findAll() {
		return suman;  
	}
}
	